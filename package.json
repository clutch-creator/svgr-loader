{
  "name": "@clutch-creator/svgr-loader",
  "version": "1.0.0",
  "description": "Turbopack svgr loader",
  "author": "Clutch Creator",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "bun@1.2.14",
  "keywords": [
    "react",
    "NextJs",
    "Turbopack",
    "clutch"
  ],
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/svg-loader.cjs",
      "import": "./dist/svg-loader.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "swc src --out-dir dist --config-file ./.swcrc --strip-leading-paths && swc src --out-dir dist --config-file ./.swcrc.esm --strip-leading-paths",
    "dev": "bun run build --watch",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@swc/helpers": "^0.5.17",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.3.60",
    "eslint": "^9.20.0",
    "@eslint/js": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.5.2"
  }
}
